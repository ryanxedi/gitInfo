{
    "name": "ryanxedi/git-info",
    "description": "Access your local git branch information",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RyanXedi\\GitInfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RyanXedi\\GitInfo\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Ryan Howard",
            "email": "ryan@xedi.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RyanXedi\\GitInfo\\GitInfoServiceProvider"
            ],
            "aliases": {
                "GitInfo": "RyanXedi\\GitInfo\\Facades\\GitInfo"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
